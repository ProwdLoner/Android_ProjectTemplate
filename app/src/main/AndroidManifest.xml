<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prowd_android_template">
    <!-- (필요 디바이스 기능 설정) -->
    <!-- <uses-feature -->
    <!-- android:name="android.hardware.camera" -->
    <!-- android:required="true" /> -->
    <!-- (필요 권한 설정) -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Application">
        <activity
            android:name=".activity_set.activity_basic_camera2_api_sample.ActivityBasicCamera2ApiSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_basic_header_footer_recycler_view_sample.ActivityBasicHeaderFooterRecyclerViewSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_multi_type_item_recycler_view_sample.ActivityMultiTypeItemRecyclerViewSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_basic_horizontal_recycler_view_sample.ActivityBasicHorizontalRecyclerViewSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_basic_vertical_recycler_view_sample.ActivityBasicVerticalRecyclerViewSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_permission_sample.ActivityPermissionSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_system_camera_sample.ActivitySystemCameraSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_camera_sample_list.ActivityCameraSampleList"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_jni_sample.ActivityJniSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_network_recycler_view_sample_editor.ActivityNetworkRecyclerViewSampleEditor"
            android:exported="false"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".activity_set.activity_basic_tab_layout_sample.ActivityBasicTabLayoutSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_basic_bottom_sheet_navigation_sample.ActivityBasicBottomSheetNavigationSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_view_pager_sample_list.ActivityViewPagerSampleList"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_network_recycler_view_sample.ActivityNetworkRecyclerViewSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_recycler_view_sample_list.ActivityRecyclerViewSampleList"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_dialog_sample.ActivityDialogSample"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_home.ActivityHome"
            android:exported="false" />
        <activity
            android:name=".activity_set.activity_init.ActivityInit"
            android:exported="true"
            android:theme="@style/Theme.ActivityInit">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.example.prowd_android_template"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>